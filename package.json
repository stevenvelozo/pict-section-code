{
    "name": "pict-section-code",
    "version": "1.0.0",
    "description": "Pict code editor section wrapping CodeJar",
    "main": "source/Pict-Section-Code.js",
    "scripts": {
        "start": "node source/Pict-Section-Code.js",
        "build": "npx quack build",
        "test": "npx mocha -u tdd -R spec",
        "tests": "npx mocha -u tdd --exit -R spec --grep",
        "coverage": "npx nyc --reporter=lcov --reporter=text-lcov npx mocha -- -u tdd -R spec",
        "docs": "npx quack prepare-docs ./docs -d ./modules",
        "docs-serve": "npx quack docs-serve ./docs",
        "example": "npx quack examples"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stevenvelozo/pict-section-code.git"
    },
    "author": "steven velozo <steven@velozo.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/stevenvelozo/pict-section-code/issues"
    },
    "homepage": "https://github.com/stevenvelozo/pict-section-code#readme",
    "devDependencies": {
        "browser-env": "^3.3.0",
        "codejar": "^4.3.0",
        "pict": "^1.0.350",
        "quackage": "^1.0.55"
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "package": "./package.json",
        "reporter": "spec",
        "slow": "75",
        "timeout": "5000",
        "ui": "tdd",
        "watch-files": [
            "source/**/*.js",
            "test/**/*.js"
        ],
        "watch-ignore": [
            "lib/vendor"
        ]
    },
    "dependencies": {
        "pict-view": "^1.0.66"
    }
}
